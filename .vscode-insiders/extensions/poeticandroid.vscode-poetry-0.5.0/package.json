{
	"name": "vscode-poetry",
	"displayName": "Poetry",
	"description": "Support for the Poetry programming language",
	"icon": "images/Poetree_128.png",
	"version": "0.5.0",
	"publisher": "poeticAndroid",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FantasyInternet/vscode-poetry.git"
	},
	"bugs": {
		"url": "https://github.com/FantasyInternet/vscode-poetry/issues"
	},
	"homepage": "https://github.com/FantasyInternet/vscode-poetry#readme",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:poetry"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "poetry",
				"aliases": [
					"Poetry",
					"poetry"
				],
				"extensions": [
					".poem"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "poetry",
				"scopeName": "source.poetry",
				"path": "./syntaxes/poetry.tmLanguage.json"
			}
		],
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "768bb1f6-ea1e-420f-9a2c-051764b3a80d",
		"publisherId": "1e0e53a1-eabc-4ee8-979b-b8bc863f3ce9",
		"publisherDisplayName": "poetic android"
	}
}